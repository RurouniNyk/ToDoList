<%- include("header") -%>
    <div class="box" id="heading">
      <h1><%=day%></h1>
    </div>
<div class="box">
    <%for(var i=0;i<additem.length;i++){%>
        <div class="item">
          <input type="checkbox">
          <p><%=additem[i]%></p>
          </div>
      <%}%>
      <form class="item" action="/" method="post">
        <input type="text" name="addlist" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%=day%>>+</button>

      </form>
</div>
<%- include("footer") -%>
